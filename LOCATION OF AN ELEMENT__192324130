#include<stdio.h>
int main()
{
int n;
	printf("B.RISHITHA  192324130\n");
printf("Enter the size of the array: \n");
scanf("%d",&n);
int array[n];
printf("Enter the elements of the array:\n");
for(int i=0;i<n;i++)
{ scanf("%d",&array[i]);
}
	int *p,ele;
	printf("Enter the element whose location is to be displayed: \n");
	scanf("%d",&ele);
	new int *;p=&array[ele];

	printf("%d", *(p+2));
}
