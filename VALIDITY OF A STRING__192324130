#include <stdio.h>
#include <string.h>

int main() {
    char str[50];
    printf("B.RISHITHA  192324130 \n");

    printf("Enter a string: ");
    scanf("%s", str);

   
    if (str[strspn(str, "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ")]) {
        printf("Invalid string.\n");
    } else {
        printf("Valid string.\n");
    }

    return 0;
}
