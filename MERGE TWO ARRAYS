#include<stdio.h>
int main()
{
	int n1,n2,n3;
		printf("B.RISHITHA  192324130\n");
	printf("size of 1st array:");
	scanf("%d",&n1);
	int a[n1];
	printf("Enter elements:");
	for(int i=0;i<n1;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("size of 2nd array:");
	scanf("%d",&n2);
	int b[n2];int c[10];
	printf("enter elements:");
	for(int i=0;i<n2;i++)
	{
		scanf("%d",&b[i]);
	}
	n3=n1+n2;
	for(int i=0;i<n1;i++)
	{
		c[i]=a[i];
	}
	for(int i=0;i<n2;i++)
	{
		c[n1+i]=b[i];
	}
	printf("Merged array:");
	for(int i=0;i<n3;i++)
	{
		printf("%d",c[i]);
		printf("\t");
	}
	return 0;
}
