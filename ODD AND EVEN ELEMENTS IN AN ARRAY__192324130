#include <stdio.h>
int main() 
{
    int n;
    printf("B.RISHITHA  192324130\n");
    printf("Enter number of elements in the array: ");
    scanf("%d", &n);
 
    int arrayy[n];
     printf("Enter %d elements in the array: ",n);
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arrayy[i]);
    }
     printf("Even numbers in the array are: ");
    for(int i=0;i<n;i++)
    {
        if(arrayy[i]%2==0)
            printf("%d ", arrayy[i]);
    }
     printf("\nOdd numbers in the array are: ");
    for(int i=0;i<n;i++)
    {
        if(arrayy[i]%2==1)
            printf("%d ", arrayy[i]);
    }
}
