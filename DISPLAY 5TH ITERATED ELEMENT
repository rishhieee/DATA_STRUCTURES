#include <stdio.h>

int main() {
    printf("B.RISHITHA  192324130 \n");
	int n;
    printf("Enter the size of the array: \n");
    scanf("%d", &n);
    int arr[n];
    printf("Enter the elements of the array: \n");
    for(int i=0;i<n;i++)
    {scanf("%d",&arr[i]);
	}


    for(int i = 0; i < n; i++) {
        if (i == 4) {
            printf("5th iterated element: %d\n", arr[i]);
        }
    }

    return 0;
}
